$("[data-fancybox]").fancybox({
    autoFocus: false,
    beforeShow: function (instance, slide) {
        $('html,body').addClass('hideOverflow');
    },
    beforeClose: function (instance, slide) {
        $('html,body').removeClass('hideOverflow');
    },
    afterLoad: function () {
        if ($('document').find('.fancybox-slide--video').length == 0 && $(window).width() > 1024) {
            setTimeout(function () {
                var posR = ($(window).width() - ($('.fancybox-content').position().left + $('.fancybox-content').outerWidth(true))),
                    posT = $('.fancybox-content').position().top;
                $('.fancybox-toolbar button[data-fancybox-close]').css({ right: posR, top: posT, transform: 'translate(100%, 0)' });
            }, 300);
        } else {
            setTimeout(function () {
                var posR = ($(window).width() - ($('.fancybox-content').position().left + $('.fancybox-content').outerWidth(true))),
                    posT = $('.fancybox-content').position().top;
                $('.fancybox-toolbar button[data-fancybox-close]').css({ right: posR, top: posT, transform: 'translate(0, -100%)' });
            }, 300);

        }
    }
});

var clList = ['.fadeIn, .fadeUp, .fadeDown'];

function animateClass() {
    setTimeout(function () {
        clList.forEach(function (item, index) {
            var anim = document.querySelectorAll(item);
            for (var i = 0, nbs = anim.length; i < nbs; i++) {
                var animELe = anim[i];
                var rect = animELe.getBoundingClientRect();
                var isVisible = ((rect.top - window.innerHeight) < 0 && (rect.bottom) > -50) ? true : false;
                if (isVisible) {
                    animELe.classList.add('animate');
                }
                else {
                    animELe.classList.remove('animate');
                }
            }
        });
    }, 100);
}
animateClass();
const translate = document.querySelectorAll(".translate");
const big_title = document.querySelector(".pgTitle");
//const header = document.querySelector("header");
