          }else if(media=="linkedin"){
          if(sharepagePath !=null)
          {
          shareURL="https://www.linkedin.com/shareArticle?mini=true&url="+sharepagePath+"&amp;title="+title;
                            window.open(shareURL,"_blank","height=485,width=700");
             window.open(shareURL,"_blank","height=485,width=700");
          }
          else{
           shareURL="https://www.linkedin.com/shareArticle?mini=true&url="+url+"&amp;title="+title;
                  window.open(shareURL,"_blank","height=485,width=700");
          }

          }else if(media=="twitter"){
          if(sharepagePath !=null)
          {
           shareURL="https://twitter.com/intent/tweet?text="+title+"&url=" + sharepagePath;
                             window.open(shareURL,"_blank","height=485,width=700");
          }
          else
          {
           shareURL="https://twitter.com/intent/tweet?text="+title+"&url=" +url;
                             window.open(shareURL,"_blank","height=485,width=700");
          }

          }
  };


