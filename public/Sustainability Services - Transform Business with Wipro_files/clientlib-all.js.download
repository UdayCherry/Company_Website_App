function dynamicBg() {

    $('[data-bg]').each(function () {
        var ths = $(this),
            dtSrc, vdType, dtPlayOption;
        if (ths.attr('data-playonhoverloop') == "true") {
            // Play on hover in loop
            // ------------------------
            // data-play-on-hover-loop="true | false"
            dtPlayOption = ' loop';
            ths.parent().addClass('playonhoverloop playonhover');
        }
        if (ths.attr('data-playonhoveronce') == "true") {
            // Play on hover once
            // ------------------------
            // data-play-on-hover-once="true | false"
            dtPlayOption = '';
            ths.parent().addClass('playonhoveronce playonhover');
        }
        if (ths.attr('data-loopedautoplay') == "true") {
            // Looped Autoplay
            // ------------------------
            // data-looped-autoplay="true | false"
            dtPlayOption = ' autoplay loop';
            ths.parent().addClass('loopedautoplay');
        }
        if (ths.attr('data-autoplayonce') == "true") {
            // Autoplay Once play again
            // ------------------------
            // data-autoplay-once="true | false"
            dtPlayOption = ' autoplay';
            ths.parent().addClass('autoplayonce');
        }
        if (ths.attr('data-autoplayonce') == "true" && ths.parent('.banner').hasClass('banner_style2')) {
            // Autoplay Once play again
            // ------------------------
            // data-autoplay-once="true | false"
            dtPlayOption = '';
            ths.parent().addClass('autoplayonce');
        }


        if (ths.attr('data-layer-switch') == 'true') {
            var bgLayerStyle = '';
            var thisEle = ths.find('span');
            if (ths.attr('data-layer-bgcolor') != "false" && ths.attr('data-layer-bgcolor') != undefined) {
                ths.find('span')[0].style.setProperty("--bgLayerColor", ths.attr('data-layer-bgcolor'));
            }
            if (ths.attr('data-layer-opacity') != "false" && ths.attr('data-layer-opacity') != undefined) {
                ths.find('span')[0].style.setProperty("--bglayerOpacity", ths.attr('data-layer-opacity'));
            }

            ths.find('span').show();
            ths.parent().addClass('bgLayerOn');
        }
        else {
            ths.find('span').hide();
        }

        if (ths.attr('data-bg') == 'image') {
            if ($(window).width() >= 1366) {
                //desktop
                ths.css({ background: 'url(' + ths.attr('data-bg-desktop') + ') repeat center / cover' })
            }
            if ($(window).width() < 1366 && $(window).width() > 767) {
                //Tablet
                ths.css({ background: 'url(' + ths.attr('data-bg-tablet') + ') repeat center / cover' })
            }
            if ($(window).width() < 768) {
                //mobile
                ths.css({ background: 'url(' + ths.attr('data-bg-mobile') + ') repeat center / cover' })
            }
        }
        if (ths.attr('data-bg') == 'video') {
            dtSrc = ths.attr('data-video');
            vdType = ths.attr('data-vdtype');
            dtPoster = ths.attr('data-poster');
            // dtPlayonhover = ths.attr('data-playonhover');
            ths.css({ background: 'url(' + dtPoster + ') repeat center / cover' });

            // console.log(dtAutoplay)
            if (ths.parent().children().hasClass('SustainPopup')) {
                $('.playButton.playVideo.SustainPopup').css('display', 'block');
            }
            if (window.location.href.indexOf('editor.html') < 0) {
                // April 2023 - removed width check that prevented videos loading on tablets and mobiles, added check to prevent multiple re-downloads
                //if ($(window).width() > 1024) { 
                if (ths.find('video').length < 1) { 
                    $.ajax({
                        url: dtSrc,
                    }).done(function (data, textStatus, jqXHR) {
                        dtPoster = ths.attr('data-poster');

                        dtId = ths.attr('data-videoId') != undefined ? ths.attr('data-videoId') : '';

                        if (ths.parent('.banner').hasClass('banner_style2') && (ths.parent('.banner').hasClass('autoplayonce') || ths.parent('.banner').hasClass('loopedautoplay'))) {
                            dtId = 'bannerVid';
                        }
                        if (ths.attr('data-playonhoverloop') == "true") {
                            dtId = 'playonhoverloop';
                        }
                        if (ths.attr('data-playonhoveronce') == "true") {
                            dtId = 'playonhoveronce';
                        }
                        if (ths.find('video').length < 1) {
                            ths.append('<video muted preload poster="' + dtPoster + '" src="' + dtSrc + '#t=0.1" ' + dtPlayOption + ' id="' + dtId + '"></video>');
                            //FIXME there's an empty <span> block that is in the html of a bunch of components. can it be removed? 
                            //in the mean time, check if it's empty and hide it
                            if (ths.find('span').length == 1) {
                                var localSpanEl = ths.find('span')[0];
                                localSpanEl.style.display="none";
                            }
                        }
                        if (('.playonhover').length) {
                            var figure = $(".playonhover").hover(hoverVideo, hideVideo);
                            function hoverVideo(e) {
                                if ($(this).find('video').length) {
                                    $(this).find('video', this).get(0).play();
                                }
                            }
                            function hideVideo(e) {
                                if ($(this).find('video').length) {
                                    $(this).find('video', this).get(0).pause()
                                    $(this).find('video', this).get(0).currentTime = 0;
                                }
                            }
                        }

                        if (ths.parent('.banner').hasClass('banner_style2') && (ths.parent('.banner').hasClass('autoplayonce') || ths.parent('.banner').hasClass('loopedautoplay'))) {

                            var banVid = document.getElementById("bannerVid");
                            if (!ths.parent().children().hasClass('SustainPopup')) {
                                banVid.oncanplay = function () {
                                    banVid.play();
                                };
                            }
                            banVid.onended = function () {
                                $('.playVideo').fadeIn().on('click', function () {
                                    if (!ths.parent().children().hasClass('SustainPopup')) {
                                        banVid.play();
                                        ths.fadeOut();
                                    }
                                });
                            };
                        }
                    }).always(function () {
                        $('.loader').fadeOut().parents('body').removeClass('loading');
                    });
                } else {
                    $('.loader').fadeOut().parents('body').removeClass('loading');
                }
            }

        }
    })

}
$(document).ready(function () {
    var isSafari = !!navigator.userAgent.match(/Version\/[\d\.]+.*Safari/);
    var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;

    if (isSafari && iOS) {
        $('body').addClass('safariIos')
    }
    dynamicBg();
    // if($(window).height() < ($('header').outerHeight() + $('.banner:first-child').outerHeight() )) {
    $('.sectionContainer > .banner:first-child').css({ margin: $('header').outerHeight(true) + 'px 0 0' });
    // }
    $(".circle-container").each(function () {
        if ($(this).parents('.textComponent').hasClass('circularAnimation')) {
            if ($(this).hasClass('circle-container__title')) {
                var paramHeight = $(this).parents(".circularAnimation").find(".title").outerHeight(true);
                $(this).parents(".circularAnimation").find('.circle-container').css({ "width": (2 * paramHeight) + "px", "height":  (2 * paramHeight) + "px" });
                $(this).parents(".circularAnimation").find('.circle-container').css({ "top": (-1 / 2 * paramHeight) + "px", "right":  (-1 * paramHeight) + "px" });
                $(this).parents(".circularAnimation").find(".title").css({ "transform": "translateX(-" + (paramHeight / 2) / 2 + ")" });
            } else {
                var paramHeight = $(this).parents(".circularAnimation").find(".text").outerHeight(true);
                $(this).parents(".circularAnimation").find('.circle-container').css({ "width": (paramHeight) + "px", "height": (paramHeight) + "px" });
                $(this).parents(".circularAnimation").find(".text").css({ "transform": "translateX(-" + (paramHeight / 2) / 2 + ")" });
            }
        }
    });
})
