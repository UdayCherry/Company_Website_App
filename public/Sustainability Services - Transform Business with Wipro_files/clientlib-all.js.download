$(window).on('load', function () {
    var hashVal = window.location.hash.substring(1);
    var hashVal2 = hashVal.split('?')[0];
    if (hashVal2 != "") {
        $('html,body').animate({ scrollTop: $('#' + hashVal2).offset().top - 50 }, 500);
    }
});

$(window).on('load', function () {
    let isErrorPage = $('body').data('search') === '/content/nexus/en/error-404';
    let start = 5; 
    if (isErrorPage) {
        let site = window.location.host;
        let homepage = site.includes('author') ? window.location.origin + '/content/nexus/en.html?wcmmode=disabled' : window.location.origin;
        let text = document.getElementById('error-redirect');
        let message = text.innerHTML;
        let interval = setInterval(function () {
            text.style.display = 'unset';
            text.innerHTML = message.replace('{page}', `<i>${site}</i>`).replace('{num}', start);
            start -= 1;
            if (start < 0) {
                clearInterval(interval)
                if (window.CQ && window.CQ.WCM.isEditMode("true")) return;
                window.location.replace(homepage);
            }
        }, 1000)
    }
});
