$(document).ready(function () {
    if ($(window).height() <= 1366 && $(window).width() <= 1024) {
        $('.bg-trees').attr('src', '/content/dam/nexus/staticsites/sustainability/assets/images/repeat_background/bgTrees.bottom-tabs.jpg');
        $('.netZero').find('.bg').removeClass('hide');
    }
    if ($(window).width() <= 800) {
        $('.bg-trees').attr('src', '/content/dam/nexus/staticsites/sustainability/assets/images/repeat_background/bgTrees.bottom-tabs.jpg');
        $('.contentBg .img7').append('<img alt="" class="bg-trees tabBg" src="/content/dam/nexus/staticsites/sustainability/assets/images/repeat_background/bgTrees.bottom-tabs.jpg"><img alt="" class="bg-trees tabBg" src="/content/dam/nexus/staticsites/sustainability/assets/images/repeat_background/bgTrees.bottom-tabs.jpg"><img alt="" class="bg-trees tabBg" src="/content/dam/nexus/staticsites/sustainability/assets/images/repeat_background/bgTrees.bottom-tabs.jpg">');
    }
    // if ($(window).width() <= 414) {
    // }
    if (!($('.banner').hasClass('banner--no-top-padding'))) {
        $('.banner').css({ paddingTop: $('header').outerHeight(true) + 'px' });
    }
    $('.sky,.clouds').css({ top: $('header').outerHeight(true) + 'px' });

    if ($(window).width() <= 767) {
        $('.slide.hbanner1').attr('style', 'margin-top: ' + $('.sub-menu').height() + 'px');
    }
})
$(window).on('load', function () {
    $('.loader').fadeOut().parents('body').removeClass('loading');
    if ($(window).width() <= 800 && $('.netZero').length > 0) {
        $('.backgroundAnimation').css({ height: ($('.netZero').offset().top + $('.netZero').outerHeight(true)) });
    }
    animateClass();
}).on('scroll', function () {
    animateClass();
    // parallax scrolling

    $(window).scrollTop() > 50 ? $('.scrollDown').css({ opacity: 0, transition: 'opacity 0.3s ease' }) : $('.scrollDown').css({ opacity: 1, transition: 'opacity 0.3s ease' })
    let scroll = window.pageYOffset;
    if ($(window).width() <= 800 && $('.netZero').length > 0) {
        $('.backgroundAnimation').css({ height: ($('.netZero').offset().top + $('.netZero').outerHeight(true)) });
    }
    if ($(window).width() > 767) {
        translate.forEach(element => {
            let speed = element.dataset.speed;
            let xy = document.getElementsByClassName('playButton')[0].getBoundingClientRect().height / 2;
            element.classList.contains('playButton')
                ? element.style.transform = `translate(-50%,${scroll * speed - xy}px)`
                : element.style.transform = `translateY(${scroll * speed}px)`;
        });
    }
    //big_title.style.opacity = -scroll / (header_height / 1.5) + 1;
}).on('resize', function () {
    if (!($('.banner').hasClass('banner--no-top-padding'))) {
        $('.banner').css({ padding: $('header').outerHeight(true) + 'px 0 0' });
    }
    $('.sky,.clouds').css({ top: $('header').outerHeight(true) + 'px' });
});

